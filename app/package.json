{
  "name": "gas-me-up",
  "version": "1.0.23",
  "scripts": {
    "start": "expo start --dev-client",
    "start:dev": "expo start --dev-client",
    "start:prod": "expo start --no-dev",
    "client": "expo start",
    "server": "cd server && npm run start-full",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "build": "npm run setup-prod; eas build",
    "build:dev": "npm run setup-dev; eas build --profile development",
    "build:devlocal": "npm run setup-dev; eas build --profile development --platform ios --local",
    "build:sim": "npm run setup-dev; eas build --profile simulator --platform ios",
    "bump": "standard-version",
    "publish": "eas submit --platform ios",
    "publish:expo": "eas update",
    "publish:android": "eas submit --platform android",
    "publish-functions": "firebase deploy",
    "check-types": "npx tsc",
    "push:secrets": "eas secret:push --scope project --env-file .env --force",
    "lint": "npx eslint './src/' 'App.tsx'",
    "lint:fix": "npx eslint --fix './src/' 'App.tsx'",
    "lint-client": "npx eslint './src/' 'App.tsx'",
    "install-server": "cd ../server && npm install",
    "test-server": "cd ../server && npm test",
    "setup-dev": "cp GoogleService-Info-Dev.plist GoogleService-Info.plist",
    "setup-prod": "cp GoogleService-Info-Prod.plist GoogleService-Info.plist"
  },
  "dependencies": {
    "@expo-google-fonts/inter": "^0.2.2",
    "@expo-google-fonts/rubik": "^0.2.2",
    "@expo/vector-icons": "^14.0.2",
    "@invertase/react-native-apple-authentication": "^2.2.2",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-firebase/analytics": "^21.6.2",
    "@react-native-firebase/app": "^21.6.2",
    "@react-native-firebase/auth": "^21.6.2",
    "@react-native-firebase/remote-config": "^21.6.2",
    "@react-navigation/bottom-tabs": "^6.3.1",
    "@react-navigation/native": "^6.0.10",
    "@react-navigation/stack": "^6.2.3",
    "deprecated-react-native-prop-types": "^3.0.1",
    "dotenv": "^16.0.3",
    "expo": "^52.0.0",
    "expo-apple-authentication": "~7.1.3",
    "expo-auth-session": "~6.0.2",
    "expo-build-properties": "~0.13.2",
    "expo-checkbox": "~4.0.1",
    "expo-constants": "~17.0.4",
    "expo-dev-client": "~5.0.9",
    "expo-device": "~7.0.2",
    "expo-font": "~13.0.3",
    "expo-linear-gradient": "~14.0.2",
    "expo-linking": "~7.0.4",
    "expo-location": "~18.0.5",
    "expo-notifications": "^0.29.12",
    "expo-random": "~13.2.0",
    "expo-splash-screen": "~0.29.20",
    "expo-status-bar": "~2.0.1",
    "expo-task-manager": "~12.0.4",
    "expo-updates": "~0.26.12",
    "expo-web-browser": "~14.0.2",
    "firebase": "^11.2.0",
    "md5": "^2.3.0",
    "npm-force-resolutions": "^0.0.10",
    "prop-types": "^15.8.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-firebase-hooks": "^5.0.3",
    "react-native": "0.76.6",
    "react-native-autocomplete-input": "^5.5.2",
    "react-native-dropdown-picker": "^5.4.4",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-maps": "1.18.0",
    "react-native-numeric-input": "github:BlueRacoon/react-native-numeric-input",
    "react-native-paper": "^5.1.4",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-uuid": "^2.0.1",
    "react-native-web": "~0.19.6",
    "throttle-debounce": "^5.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@types/md5": "^2.3.2",
    "@types/react": "~18.3.12",
    "@types/react-native-autocomplete-input": "^5.1.1",
    "@types/throttle-debounce": "^5.0.0",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "eslint": "^8.15.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.5.0",
    "eslint-plugin-react-native": "^4.0.0",
    "standard-version": "^9.5.0",
    "typescript": "^5.1.3"
  },
  "private": true,
  "proxy": "http://localhost:3001",
  "rnpm": {
    "assets": [
      "./assets/fonts/"
    ]
  }
}
