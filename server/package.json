    "@types/jest": "^28.1.1",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.3.6",
    "chai-http": "^4.3.0",
    "jest": "^28.1.0",
    "sinon-chai": "^3.7.0",
    "superagent": "^7.1.3",
    "supertest": "^6.2.3"
